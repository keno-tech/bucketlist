generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BucketList {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  imageUrl    String?
  achieved    Boolean  @default(false)
  createdAt   DateTime @default(now())
}
